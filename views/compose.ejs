<%- include("partials/header") -%> 
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="exampleInputEmail1">Title:</label>
        <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name="postTitle" autocomplete="off">
    </div>
    <div class="form-group">
        <label for="exampleFormControlTextarea1">Post:</label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="5" cols="30" name="postBody"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%> 